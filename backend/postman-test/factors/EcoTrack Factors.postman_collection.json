{
	"info": {
		"_postman_id": "f2d47eb5-2389-4b9a-b3cc-0325abac0e3a",
		"name": "EcoTrack Factors API",
		"description": "API testing collection for EcoTrack emission factors endpoint",
		"schema": "https://schema.getpostman.com/json/collection/v2.0.0/collection.json",
		"_exporter_id": "35651548"
	},
	"item": [
		{
			"name": "Factors/Utilities",
			"item": [
				{
					"name": "Get Factors - Success",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has success message\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('message');",
									"    pm.expect(jsonData.message).to.eql('Emission factors retrieved successfully');",
									"});",
									"",
									"pm.test(\"Response has factors data\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('factors');",
									"    pm.expect(jsonData.factors).to.be.an('object');",
									"});",
									"",
									"pm.test(\"Factors has all required categories\", function () {",
									"    var factors = pm.response.json().factors;",
									"    pm.expect(factors).to.have.property('transport');",
									"    pm.expect(factors).to.have.property('food');",
									"    pm.expect(factors).to.have.property('energy');",
									"    pm.expect(factors).to.have.property('shopping');",
									"});",
									"",
									"pm.test(\"Transport factors are valid array\", function () {",
									"    var transport = pm.response.json().factors.transport;",
									"    pm.expect(transport).to.be.an('array').that.is.not.empty;",
									"    pm.expect(transport[0]).to.have.property('key');",
									"    pm.expect(transport[0]).to.have.property('label');",
									"    pm.expect(transport[0]).to.have.property('unit');",
									"    pm.expect(transport[0]).to.have.property('factor');",
									"    pm.expect(transport[0].factor).to.be.a('number');",
									"});",
									"",
									"pm.test(\"Food factors are valid array\", function () {",
									"    var food = pm.response.json().factors.food;",
									"    pm.expect(food).to.be.an('array').that.is.not.empty;",
									"    pm.expect(food[0]).to.have.property('key');",
									"    pm.expect(food[0]).to.have.property('label');",
									"    pm.expect(food[0]).to.have.property('unit');",
									"    pm.expect(food[0]).to.have.property('factor');",
									"    pm.expect(food[0].factor).to.be.a('number');",
									"});",
									"",
									"pm.test(\"Energy factors are valid array\", function () {",
									"    var energy = pm.response.json().factors.energy;",
									"    pm.expect(energy).to.be.an('array').that.is.not.empty;",
									"    pm.expect(energy[0]).to.have.property('key');",
									"    pm.expect(energy[0]).to.have.property('label');",
									"    pm.expect(energy[0]).to.have.property('unit');",
									"    pm.expect(energy[0]).to.have.property('factor');",
									"    pm.expect(energy[0].factor).to.be.a('number');",
									"});",
									"",
									"pm.test(\"Shopping factors are valid array\", function () {",
									"    var shopping = pm.response.json().factors.shopping;",
									"    pm.expect(shopping).to.be.an('array').that.is.not.empty;",
									"    pm.expect(shopping[0]).to.have.property('key');",
									"    pm.expect(shopping[0]).to.have.property('label');",
									"    pm.expect(shopping[0]).to.have.property('unit');",
									"    pm.expect(shopping[0]).to.have.property('factor');",
									"    pm.expect(shopping[0].factor).to.be.a('number');",
									"});",
									"",
									"pm.test(\"Transport factors contain expected vehicle types\", function () {",
									"    var transport = pm.response.json().factors.transport;",
									"    var keys = transport.map(item => item.key);",
									"    pm.expect(keys).to.include('car_medium_petrol');",
									"    pm.expect(keys).to.include('motorcycle_avg');",
									"    pm.expect(keys).to.include('bus_local');",
									"    pm.expect(keys).to.include('rail_national');",
									"});",
									"",
									"pm.test(\"Food factors contain expected food types\", function () {",
									"    var food = pm.response.json().factors.food;",
									"    var keys = food.map(item => item.key);",
									"    pm.expect(keys).to.include('beef_kg');",
									"    pm.expect(keys).to.include('chicken_kg');",
									"    pm.expect(keys).to.include('rice_kg');",
									"});",
									"",
									"pm.test(\"Energy factors contain grid electricity\", function () {",
									"    var energy = pm.response.json().factors.energy;",
									"    var keys = energy.map(item => item.key);",
									"    pm.expect(keys).to.include('grid_uk');",
									"});",
									"",
									"pm.test(\"Shopping factors contain expected item types\", function () {",
									"    var shopping = pm.response.json().factors.shopping;",
									"    var keys = shopping.map(item => item.key);",
									"    pm.expect(keys).to.include('clothing_item');",
									"    pm.expect(keys).to.include('electronics_item');",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{token}}"
							}
						],
						"url": "{{base_url}}/factors"
					},
					"response": []
				},
				{
					"name": "Get Factors - Unauthorized",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 401\", function () {",
									"    pm.response.to.have.status(401);",
									"});",
									"",
									"pm.test(\"Response has unauthorized error\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('message');",
									"    pm.expect(jsonData.message).to.eql('Not authorized to access this route');",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": "{{base_url}}/factors"
					},
					"response": []
				}
			]
		}
	],
